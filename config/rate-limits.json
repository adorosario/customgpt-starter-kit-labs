{
    "identityOrder": ["jwt-sub", "session-cookie", "ip"],
    "jwtSecret": "your-jwt-secret-key-here",
    "limits": {
      "global": {
        "minute": 100,
        "hour": 1000,
        "day": 10000,
        "month": 300000
      }
    },
    "routesInScope": [
      "/api/proxy/projects",
      "/api/proxy/user"
    ],
    "turnstileEnabled": true,
    "turnstile": {
      "enabled": true,
      "bypassAuthenticated": true,
      "requiredForIPUsers": true,
      "cacheDurationMinutes": 5,
      "testMode": false
    }
  }